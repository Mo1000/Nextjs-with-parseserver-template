    "glob": "^11.0.2",
    "ioredis": "^5.6.1",
    "next": "15.1.8",
    "parse": "^6.1.1",
    "parse-dashboard": "^7.1.0",
    "parse-server": "^8.2.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "rimraf": "^6.0.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@types/express": "^5.0.2",
    "@types/node": "^20",
    "@types/parse": "^3.0.9",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "concurrently": "^9.1.2",
    "cross-env": "^7.0.3",
    "eslint": "^9",
    "eslint-config-next": "15.1.8",
